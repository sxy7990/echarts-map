{
  "name": "echarts-map",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "echarts": "^4.6.0",
    "echarts-gl": "^1.1.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^6.1.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-mixed-operators": 0,
      "no-plusplus": 0,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "max-len": 0,
      "global-require": 0,
      "no-console": 0,
      "no-unused-vars": 0,
      "object-shorthand": 0,
      "no-useless-return": 0,
      "consistent-return": 0,
      "prefer-rest-params": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
